{
    "name": "claudiodekker/laravel-auth-core",
    "description": "The Laravel Auth Core Package.",
    "license": "MIT",
    "homepage": "https://github.com/claudiodekker/laravel-auth",
    "support": {
        "issues": "https://github.com/claudiodekker/laravel-auth/issues",
        "source": "https://github.com/claudiodekker/laravel-auth"
    },
    "authors": [
        {
            "name": "Claudio Dekker",
            "email": "claudio@ubient.net"
        }
    ],
    "require": {
        "php": "~8.1.0|~8.2.0",
        "ext-json": "*",
        "bacon/bacon-qr-code": "^2.0",
        "claudiodekker/word-generator": "^1.0",
        "laravel/framework": "^9.33|^10.0",
        "nyholm/psr7": "^1.5",
        "pragmarx/google2fa": "^8.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "web-auth/webauthn-lib": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^7.24|^8.2",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "ClaudioDekker\\LaravelAuth\\": "src/",
            "ClaudioDekker\\LaravelAuth\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClaudioDekker\\LaravelAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClaudioDekker\\LaravelAuth\\LaravelAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
