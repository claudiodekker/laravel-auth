{
    "name": "claudiodekker/laravel-auth-bladebones",
    "description": "Rich (yet bare) authentication scaffolding for any blade-based Laravel application.",
    "license": "MIT",
    "homepage": "https://github.com/claudiodekker/laravel-auth",
    "support": {
        "issues": "https://github.com/claudiodekker/laravel-auth/issues",
        "source": "https://github.com/claudiodekker/laravel-auth"
    },
    "keywords": [
        "laravel",
        "auth",
        "authentication",
        "scaffolding",
        "blade",
        "barebones",
        "template"
    ],
    "authors": [
        {
            "name": "Claudio Dekker",
            "email": "claudio@ubient.net"
        }
    ],
    "require": {
        "php": "~8.1.0|~8.2.0",
        "ext-json": "*",
        "laravel/framework": "^9.0|^10.0",
        "claudiodekker/laravel-auth-core": "^0.1.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.5|^7.0",
        "phpunit/phpunit": "^8.4|^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "ClaudioDekker\\LaravelAuthBladebones\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClaudioDekker\\LaravelAuthBladebones\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClaudioDekker\\LaravelAuthBladebones\\LaravelAuthBladebonesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
